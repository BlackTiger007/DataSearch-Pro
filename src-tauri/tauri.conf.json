			{
				"title": "DataSearch-Pro",
				"label": "main",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": null
		}
	},
	"plugins": {
		"updater": {
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDhBMEZCMEZCQjgxQTI3MTAKUldRUUp4cTQrN0FQaXJvQ0ZNcDNYdE51YmNRdXB2NnpUbElZRWJyNUlTSUpYNk44R3ljNjlZRUMK",
			"endpoints": [
				"https://github.com/BlackTiger007/DataSearch-Pro/releases/latest/download/latest.json"
			],
			"windows": {
				"installMode": "passive"
			}
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"createUpdaterArtifacts": true,
